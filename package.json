  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/node": "^13.1.2",
    "fs-extra": "^8.1.0",
    "globby": "^10.0.1",
    "typescript": "^3.8.0-dev.20200103"
  },
  "dependencies": {}
}
